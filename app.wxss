.start {
  position: absolute;
  top: -80rpx;
  background-color: #d8d6d6;
  height: 10rpx;
  width: 10rpx;
  border-radius: 50%;
  opacity: 0;
}

.start:nth-of-type(3) {
  top: -40rpx;
  left: 20rpx;
  animation: start 5s ease 3s infinite;
}

.start:nth-of-type(4) {
  top: 50rpx;
  right: 100rpx;
  animation: start 3s ease 1s infinite;
}

.start:nth-of-type(5) {
  top: 75rpx;
  left: -50rpx;
  animation: start 4s ease 1s infinite;
}

.start:nth-of-type(6) {
  top: -30rpx;
  right: 30rpx;
  animation: start 16s ease 1s infinite;
}

.start:nth-of-type(7) {
  top: 120rpx;
  right: 50rpx;
  animation: start 7s ease infinite;
}

.start:nth-of-type(8) {
  top: 180rpx;
  left: 30rpx;
  animation: start 3s ease 1s infinite;
}

@keyframes start {
