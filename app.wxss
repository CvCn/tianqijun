#sun {
  margin-top: 80rpx;
  margin-left: 400rpx;
  color: #fe4;
  width: 150rpx;
  height: 150rpx;
  background-color: #f90;
  border-radius: 50%;
  position: relative;
  animation: run 15s ease infinite;
}

@keyframes run {
  0% {
    transform: rotate(0deg);
    color: rgba(255, 239, 68, 1);
  }

  45% {
    transform: rotate(30deg);
    color: rgba(255, 239, 68, 0.4);
  }

  80% {
    transform: rotate(-10deg);
    color: rgba(255, 239, 68, 0.7);
  }

  100% {
    transform: rotate(0deg);
    color: rgba(255, 239, 68, 1);
  }
}

#sun:before {
  content: " ";
  width: 20px;
  height: 5px;
  margin: auto;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  border-radius: 15px;
  box-shadow: 60px 0px 0 1px currentColor, -60px 0 0 1px currentColor;
}

#sun:after {
  content: " ";
  width: 5px;
  height: 20px;
  margin: auto;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  border-radius: 15px;
  box-shadow: 0px 60px 0 1px currentColor, 0 -60px 0 1px currentColor;
}

.sunny {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 5px;
  height: 20px;
  margin: auto;
  transform: rotate(45deg);
  border-radius: 15px;
  box-shadow: 0px 60px 0 1px currentColor, 0 -60px 0 1px currentColor;
}

.sunny:after {
  content: " ";
  position: absolute;
  width: 5px;
  height: 20px;
  margin: auto;
  transform: rotate(90deg);
  border-radius: 15px;
  box-shadow: 0 60px 0 1px currentColor, 0 -60px 0 1px currentColor;
}

.moon-fa {
  margin-left: 420rpx;
