/* components/detailsCard/detailsCard.wxss */
.card {
  background-color: white;
  width: 570rpx;
  border-radius: 5px;
  padding: 20rpx 30rpx;
  box-shadow: 0rpx 2rpx 7rpx 0 rgba(128, 128, 128, 0.1);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

@keyframes fadein{
  from{
    transform:scale(0, 0);
    opacity: 0;
  }
  to{
    transform:scale(1, 1);
    opacity: 1;
  }
}

@keyframes fadeout{
  from{
    transform:scale(1, 1);
    opacity: 1;
  }
  to{
    transform:scale(0, 0);
    opacity: 0;
  }
}

.card > view:first-child{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
  height: 200rpx;
  font-size: 28rpx;
  
}

.shad{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}










