.weather-item{
  display: flex;
  flex-wrap: wrap;
  padding: 10rpx 0;
  /* color: #585872; */
}

.weather-item > view{
  font-size: 26rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  height: 140rpx;
  width: 277rpx;
  padding: 10rpx 0;
  border-radius: 5px;
  background-color: white;
  margin: 10rpx 5rpx;
  /* box-shadow: 0rpx 2rpx 7rpx 0 rgba(128, 128, 128, 0.1); */
  position: relative;
  /* overflow: hidden; */
}

.weather-item > view > i{
  font-size: 40rpx;
}

.weather-item > view > view:nth-child(2){
  color: #8c8c8c79;
}

.weather-main{
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 460rpx;
}

.weather-main > view{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  padding-bottom: 20rpx;
}

.water{
  height: 0;
  max-height: 160rpx;
  width: 200rpx;
  background-color: #5ab4db;
  position: absolute;
  bottom: 0;
  border-radius: 0 0 5px 5px;
  opacity: 0.15;
  box-shadow: 0rpx 2rpx 7rpx 0 rgba(128, 128, 128, 0.6);
  display: flex;
  flex-wrap: wrap;
}
